    <style type="text/css">
        /* Điều chỉnh vị trí của ô hiển thị số trang và ô tìm kiếm */
        #example_paginate,
        #example_filter {
            margin-top: 10px; /* Điều chỉnh giảm khoảng cách với bảng */
        }

            /* Điều chỉnh kích thước của ô tìm kiếm */
            #example_filter input {
                height: 38px;
            }
        /* Add border to the table */
        #example {
            border-collapse: collapse;
            width: 100%;
            border: 1px solid #ddd;
        }

            /* Add border to table header cells */
            #example th {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

            /* Add border to table body cells */
            #example td {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

                /* Style for the checkbox column */
                #example td:first-child {
                    text-align: center;
                }

